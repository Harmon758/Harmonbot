on *:text:!caught*:#imagrill:{
  if ($2 == random) { msg # Arts has caught a wild $randomviewer $+ ! }
  elseif ($2) { msg # Arts has caught a wild $capital($2-) $+ ! }
  else { msg # Arts has caught a wild $capital($nick) $+ ! }
}

on *:text:!death*:#imagrill:{
  if ($nick isop #) && ($2 == inc) {
    inc %arts.death
    msg # Arts has died %arts.death times.
  }
  else { msg # Arts has died %arts.death times. }
}

on *:text:!ed8*:#:{
  if ($nick isop #) && ($2) {
    if ($istok(on,$2,32)) {
      set %!edate.status on 
      msg # !ed8 is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %!edate.status off 
      msg # !ed8 is off.
      return
    }
  }
  if (%!edate.status == off) { return }
  else {
    inc %edate
    msg # Arts has %edate ed8's.
  }
}

on *:text:!edate*:#:{
  if ($nick isop #) && ($2) {
    if ($istok(on,$2,32)) {
      set %!edate.status on 
      msg # !edate is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %!edate.status off 
      msg # !edate is off.
      return
    }
  }
  if (%!edate.status == off) { return }
  else {
    inc %edate
    msg # Arts has %edate edates.
  }
}

on *:text:!fail*:#:{
  inc %fail
  msg # Fail $chr(35) $+ %fail $+ . http://imgur.com/FWv07A9
}

on *:text:!sneeze*:#imagrill:{
  if (!$2 || $2 > 10 || $2 < 2 ) { msg # Bless you! }
  else {
    var %counter = $2
    var %message = Bless you!
    while (%counter > 1) {
      %message = %message Bless you!
      %counter = %counter - 1
    }
    msg # %message
  }
}

on *:text:!troll*:#imagrill:{
  if ($nick isop #) && ($2 == inc) {
    if ($3 isnum) { inc %troll $3 }
    else { inc %troll }
    msg # There have been %troll trolls.
  }
  else { msg # There have been %troll trolls. }
}

