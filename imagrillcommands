}

on $*:text:/.*\72(3)/Si:#imagrill:{ 
  if ($nick isop #) && ($2) {
    if ($istok(on,$2,32)) {
      set %:3.status on 
      msg # :3 is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %:3.status off 
      msg # :3 is off.
      return
    }
  }
  if (%:3.status == off) { return }
  else { msg # msg # http://puu.sh/iE7ik.jpg }
}

