on *:text:!caught*:#imagrill:{
  if ($2 == random) { msg # Arts has caught a wild $randomviewer $+ ! }
  elseif ($2) { msg # Arts has caught a wild $capital($2-) $+ ! }
  else { msg # Arts has caught a wild $capital($nick) $+ ! }
}

on *:text:!death*:#imagrill:{
  if ($nick isop #) && ($2 == inc) {
    inc %arts.death
    msg # Arts has died %arts.death times.
  }
  else { msg # Arts has died %arts.death times. }
}

on *:text:!ed8*:#:{
  if ($nick isop #) && ($2) {
    if ($istok(on,$2,32)) {
      set %!edate.status on 
      msg # !ed8 is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %!edate.status off 
      msg # !ed8 is off.
      return
    }
  }
  if (%!edate.status == off) { return }
  else {
    inc %edate
    msg # Arts has %edate ed8's.
  }
}

on *:text:!edate*:#:{
