    if ($istok(on,$2,32)) {
      set %!edate.status on 
      msg # !edate is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %!edate.status off 
      msg # !edate is off.
      return
    }
  }
  if (%!edate.status == off) { return }
  else {
    inc %edate
    msg # Arts has %edate edates.
  }
}

on *:text:!fail*:#:{
  inc %fail
  msg # Fail $chr(35) $+ %fail $+ . http://imgur.com/FWv07A9
}

on *:text:!sneeze*:#imagrill:{
  if (!$2 || $2 > 10 || $2 < 2 ) { msg # Bless you! }
  else {
    var %counter = $2
    var %message = Bless you!
    while (%counter > 1) {
      %message = %message Bless you!
      %counter = %counter - 1
    }
    msg # %message
  }
}

on *:text:!troll*:#imagrill:{
  if ($nick isop #) && ($2 == inc) {
    if ($3 isnum) { inc %troll $3 }
    else { inc %troll }
    msg # There have been %troll trolls.
  }
  else { msg # There have been %troll trolls. }
}

on *:text:*yawn*:#imagrill:{
  if ($nick isop #) && ($2) {
    if ($istok(on,$2,32)) {
      set %yawn.status on 
      msg # yawn is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %yawn.status off 
      msg # yawn is off.
      return
    }
  }
  if (%yawn.status == off) { return }
  else { msg # Yawwwwwn }
}

