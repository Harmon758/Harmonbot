on *:text:*:#imagrill:{
  if ($nick != pmfornudes) && ($1 == pmpls) && ($2) {
    if ($istok(on,$2,32)) {
      set %pmpls.status on 
      msg # pmpls is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %pmpls.status off 
      msg # pmpls is off.
      return
    }
  }
  if (%pmpls.status == off) { return }
  else {
    if ($nick == pmfornudes) { msg # PMForNud pls }
  }
}
