n410=    msg # $4
n411=    return msg
n412=  }
n413=  elseif (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == mod) {
n414=    return was_mod_!msg
n415=  }
n416=  else {
n417=    msg # $4
