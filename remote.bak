n298=on *:sockopen:uptime:{
n299=  if ($sockerr) { sockclose $sockname | halt }
n300=  sockwrite -n $sockname GET /hosted/uptime.php?channel=mikkiscape HTTP/1.1
n301=  sockwrite -n $sockname Host: nightdev.com $crlf $+ $crlf
n302=}
n303=on *:sockread:uptime:{
n304=  if ($sockerr) { sockclose $sockname }
n305=  var %streamodata
n306=  sockread %streamodata
n307=  if ($regex(%streamodata,(.*)minutes)) { msg #mikkiscape $regml(1) minutes }
n308=  if ($regex(%streamodata,(.*)live.)) { msg #mikkiscape Mikki is offline. }
n309=}
