n288=  var %2 = $remove($2,$chr(44))
n289=  if (!$2) { msg # Please enter xp. }
n290=  elseif (%2 isnum && %2 >= 0 && %2 < 200000001) {
n291=    var %2 = $floor(%2)
n292=    var %level = 1
n293=    var %xp = 0
n294=    while (%2 >= %xp) {
n295=      %xp = $calc(%xp * 4)
n296=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n297=      %xp = $calc(%xp / 4)
n298=      inc %level
n299=    }
n300=    %level = %level - 1
n301=    msg # $bytes(%2,bd) xp = level %level
n302=  }
n303=  elseif (%2 isnum) { msg # You can't have that much xp! }
n304=  else { msg # Syntax error. }
n305=}
n306=on *:text:!xpbetween*:#:{
n307=  if ($2 isnum && $2 >= 1 && $2 < 127 && $3 isnum && $3 >= 1 && $3 < 127 && $2 < $3) {
n308=    var %2 = $floor($2)
n309=    var %3 = $floor($3)
n310=    var %level = 1
n311=    var %xp = 0
n312=    var %startxp = 0
n313=    var %betweenxp = 0
n314=    while (%level != %3) {
n315=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n316=      inc %level
n317=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n318=    }
n319=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n320=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n321=  }
n322=  else { msg # Syntax error. }
n323=}
