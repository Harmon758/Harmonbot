n360=alias timeleft {
n361=  var %start = $1, %interval = $2, %timeleft
n362=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n363=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n364=  return %timeleft
n365=}
n366=
n367=alias my_duration {
n368=  var %secs = $1, %my_duration
n369=  if (%secs > 604800) {
n370=    %my_duration = %my_duration $floor($calc(%secs / 604800)) weeks
n371=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n372=  }
n373=  if (%secs > 86400) {
n374=    %my_duration = %my_duration $floor($calc(%secs / 86400)) days
n375=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n376=  }
n377=  if (%secs > 3600) {
n378=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hours
n379=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n380=  }
n381=  if (%secs > 60) {
n382=    %my_duration = %my_duration $floor($calc(%secs / 60)) minutes
n383=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n384=  }
n385=  if (%secs != 0) {
n386=    %my_duration = %my_duration %secs seconds
n387=  }
n388=  return %my_duration
n389=}
