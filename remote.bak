n419=alias my_duration {
n420=  var %secs = $1, %my_duration
n421=  if (%secs > 604800) {
n422=    %my_duration = %my_duration $floor($calc(%secs / 604800)) weeks
n423=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n424=  }
n425=  if (%secs > 86400) {
n426=    %my_duration = %my_duration $floor($calc(%secs / 86400)) days
n427=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n428=  }
n429=  if (%secs > 3600) {
n430=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hours
n431=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n432=  }
n433=  if (%secs > 60) {
n434=    %my_duration = %my_duration $floor($calc(%secs / 60)) minutes
n435=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n436=  }
n437=  if (%secs != 0) {
n438=    %my_duration = %my_duration %secs seconds
n439=  }
n440=  return %my_duration
n441=}
