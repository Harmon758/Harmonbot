n311=alias timeleft {
n312=  var %start = $1, %interval = $2, %timeleft
n313=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n314=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n315=  return %timeleft
n316=}
