n367=on *:text:!uptime*:#:{
n368=  set %channel $chan
n369=  sockclose uptime
n370=  sockopen uptime nightdev.com 80
n371=}
n372=on *:sockopen:uptime:{
n373=  if ($sockerr) { sockclose $sockname | halt }
n374=  sockwrite -n $sockname GET /hosted/uptime.php?channel= $+ $mid(%channel,2-) HTTP/1.1
n375=  sockwrite -n $sockname Host: nightdev.com $crlf $+ $crlf
n376=}
n377=on *:sockread:uptime:{
n378=  if ($sockerr) { sockclose $sockname }
n379=  var %streamodata
n380=  sockread %streamodata
n381=  if ($regex(%streamodata,(.*)minutes)) { msg %channel $regml(1) minutes }
n382=  if ($regex(%streamodata,(.*)live.)) { msg %channel $capital($mid(%channel,2-)) is offline. }
n383=}
n384=
n385=alias timeleft {
n386=  var %start = $1, %interval = $2, %timeleft
n387=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n388=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n389=  return %timeleft
n390=}
n391=
n392=alias my_duration {
n393=  var %secs = $1, %my_duration
n394=  if (%secs > 604800) {
n395=    %my_duration = %my_duration $floor($calc(%secs / 604800)) weeks
n396=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n397=  }
n398=  if (%secs > 86400) {
n399=    %my_duration = %my_duration $floor($calc(%secs / 86400)) days
n400=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n401=  }
n402=  if (%secs > 3600) {
n403=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hours
n404=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n405=  }
n406=  if (%secs > 60) {
n407=    %my_duration = %my_duration $floor($calc(%secs / 60)) minutes
n408=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n409=  }
n410=  if (%secs != 0) {
n411=    %my_duration = %my_duration %secs seconds
n412=  }
n413=  return %my_duration
n414=}
