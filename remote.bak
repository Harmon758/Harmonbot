n299=    var %level = 1
n300=    var %xp = 0
n301=    var %startxp = 0
n302=    var %betweenxp = 0
n303=    while (%level != %3) {
n304=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n305=      inc %level
n306=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n307=    }
n308=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n309=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n310=  }
n311=  else { msg # Syntax error. }
n312=}
