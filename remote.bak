n388=alias my_duration {
n389=  var %secs = $1, %my_duration
n390=  if (%secs > 31536000) {
n391=    %my_duration = %my_duration $floor($calc(%secs / 31536000)) year
n392=    if ($floor($calc(%secs / 31536000)) > 1) { %my_duration = %my_duration $+ s }
n393=    %secs = $calc(%secs - $floor($calc(%secs / 31536000)) * 31536000)
n394=  }
n395=  if (%secs > 604800) {
n396=    %my_duration = %my_duration $floor($calc(%secs / 604800)) week
n397=    if ($floor($calc(%secs / 604800)) > 1) { %my_duration = %my_duration $+ s }
n398=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n399=  }
n400=  if (%secs > 86400) {
n401=    %my_duration = %my_duration $floor($calc(%secs / 86400)) day
n402=    if ($floor($calc(%secs / 86400)) > 1) { %my_duration = %my_duration $+ s }
n403=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n404=  }
n405=  if (%secs > 3600) {
n406=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hour
n407=    if ($floor($calc(%secs / 3600)) > 1) { %my_duration = %my_duration $+ s }
n408=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n409=  }
n410=  if (%secs > 60) {
n411=    %my_duration = %my_duration $floor($calc(%secs / 60)) minute
n412=    if ($floor($calc(%secs / 60)) > 1) { %my_duration = %my_duration $+ s }
n413=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n414=  }
n415=  if (%secs != 0) {
n416=    %my_duration = %my_duration %secs second
n417=    if (%secs > 1) { %my_duration = %my_duration $+ s }
n418=  }
n419=  return %my_duration
n420=}
