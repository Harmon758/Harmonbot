n423=alias timeleft {
n424=  var %start = $1, %interval = $2, %timeleft
n425=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n426=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n427=  return %timeleft
n428=}
