n381=alias timeleft {
n382=  var %start = $1, %interval = $2, %timeleft
n383=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n384=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n385=  return %timeleft
n386=}
