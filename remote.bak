n412=alias timeleft {
n413=  var %start = $1, %interval = $2, %timeleft
n414=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n415=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n416=  return %timeleft
n417=}
