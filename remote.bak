n346=    var %3 = $floor($3)
n347=    var %level = 1
n348=    var %xp = 0
n349=    var %startxp = 0
n350=    var %betweenxp = 0
n351=    while (%level != %3) {
n352=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n353=      inc %level
n354=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n355=    }
n356=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n357=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n358=  }
n359=  else { msg # Syntax error. }
n360=}
