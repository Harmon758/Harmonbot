n393=alias on/off {
n394=  if ($nick isop #) && ($2) {
n395=    if ($istok(on,$2,32)) {
n396=      set % $+ $3 $+ .status. $+ $chan on
n397=      if ($5) { msg # $5 is on. }
n398=      else { msg # $3 is on. }
n399=      return on
n400=    }
n401=    if ($istok(off,$2,32)) {
n402=      set % $+ $3 $+ .status. $+ $chan off
n403=      if ($5) { msg # $5 is off. }
n404=      else { msg # $3 is off. }
n405=      return off
n406=    }
n407=    if ($istok(mod,$2,32)) {
n408=      set % $+ $3 $+ .status. $+ $chan mod
n409=      if ($5) { msg # $5 is mod only. }
n410=      else { msg # $3 is mod only. }
n411=      return mod
n412=    }
n413=  }
n414=  if (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == off) { return was_off }
n415=  elseif (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == mod) && ($nick isop #) {
n416=    msg # $4
n417=    return msg
n418=  }
n419=  elseif (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == mod) {
n420=    return was_mod_!msg
n421=  }
n422=  else {
n423=    msg # $4
n424=    return msg
n425=  }
n426=}
