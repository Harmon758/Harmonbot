n440=      if ($5) { msg # $5 is mod only }
n441=      else { msg # $3 is mod only }
n442=      return mod
n443=    }
n444=  }
n445=  if (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == off) { return was_off }
n446=  elseif (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == mod) && ($nick isop #) {
n447=    msg # $4
n448=    return msg
n449=  }
n450=  elseif (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == mod) {
n451=    return was_mod_!msg
n452=  }
n453=  else {
n454=    msg # $4
n455=    return msg
n456=  }
