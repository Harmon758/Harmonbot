n342=on *:text:!uptime*:#:{
n343=  set %channel $chan
n344=  sockclose uptime
n345=  sockopen uptime nightdev.com 80
n346=}
n347=on *:sockopen:uptime:{
n348=  if ($sockerr) { sockclose $sockname | halt }
n349=  sockwrite -n $sockname GET /hosted/uptime.php?channel= $+ $mid(%channel,2-) HTTP/1.1
n350=  sockwrite -n $sockname Host: nightdev.com $crlf $+ $crlf
n351=}
n352=on *:sockread:uptime:{
n353=  if ($sockerr) { sockclose $sockname }
n354=  var %streamodata
n355=  sockread %streamodata
n356=  if ($regex(%streamodata,(.*)minutes)) { msg %channel $regml(1) minutes }
n357=  if ($regex(%streamodata,(.*)live.)) { msg %channel $capital($mid(%channel,2-)) is offline. }
n358=}
