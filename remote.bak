n169=on *:text:!level*:#:{
n170=  if ($2 isnum && $2 >= 1 && $2 < 127) {
n171=    var %2 = $floor($2)
n172=    var %level = 1
n173=    var %xp = 0
n174=    while (%level != %2) {
n175=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n176=      inc %level
n177=    }
n178=    %xp = $floor($calc(%xp / 4))
n179=    msg # Level %2 = $bytes(%xp,bd) xp
n180=  }
n181=  elseif ($2 isnum && $2 > 9000) { msg # It's over 9000! }
n182=  elseif ($2 isnum && $2 == 9000) { msg # Almost there. }
n183=  elseif ($2 isnum && $2 > 126 && $2 < 9000) { msg # I was gonna calculate xp at Level $2. Then I took an arrow to the knee. }
n184=  else { msg # Level $2- does not exist or there is a syntax error. }
n185=}
