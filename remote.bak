n191=    var %3 = $floor($3)
n192=    var %level = 1
n193=    var %xp = 0
n194=    var %startxp = 0
n195=    var %betweenxp = 0
n196=    while (%level != %3) {
n197=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n198=      inc %level
n199=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n200=    }
n201=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n202=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n203=  }
n204=  else { msg # Syntax error. }
n205=}
