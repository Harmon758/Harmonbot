n328=on *:text:!xpat*:#:{
n329=  var %2 = $remove($2,$chr(44))
n330=  if (!$2) { msg # Please enter xp. }
n331=  elseif (%2 isnum && %2 >= 0 && %2 < 200000001) {
n332=    var %2 = $floor(%2)
n333=    var %level = 1
n334=    var %xp = 0
n335=    while (%2 >= %xp) {
n336=      %xp = $calc(%xp * 4)
n337=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n338=      %xp = $calc(%xp / 4)
n339=      inc %level
n340=    }
n341=    %level = %level - 1
n342=    msg # $bytes(%2,bd) xp = level %level
n343=  }
n344=  elseif (%2 isnum) { msg # You can't have that much xp! }
n345=  else { msg # Syntax error. }
n346=}
n347=on *:text:!xpbetween*:#:{
n348=  if ($2 isnum && $2 >= 1 && $2 < 127 && $3 isnum && $3 >= 1 && $3 < 127 && $2 < $3) {
n349=    var %2 = $floor($2)
n350=    var %3 = $floor($3)
n351=    var %level = 1
n352=    var %xp = 0
n353=    var %startxp = 0
n354=    var %betweenxp = 0
n355=    while (%level != %3) {
n356=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n357=      inc %level
n358=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n359=    }
n360=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n361=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n362=  }
n363=  else { msg # Syntax error. }
n364=}
