n379=alias timeleft {
n380=  var %start = $1, %interval = $2, %timeleft
n381=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n382=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n383=  return %timeleft
n384=}
