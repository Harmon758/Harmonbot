n200=on *:text:!level*:#:{
n201=  if (!$2) { msg # Please enter a level. }
n202=  elseif ($2 isnum && $2 >= 1 && $2 < 127) {
n203=    var %2 = $floor($2)
n204=    var %level = 1
n205=    var %xp = 0
