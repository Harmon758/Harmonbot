n312=alias my_duration {
n313=  var %secs = $1, %my_duration
n314=  if (%secs > 604800) {
n315=    %my_duration = %my_duration $floor($calc(%secs / 604800)) weeks
n316=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n317=  }
n318=  if (%secs > 86400) {
n319=    %my_duration = %my_duration $floor($calc(%secs / 86400)) days
n320=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n321=  }
n322=  if (%secs > 3600) {
n323=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hours
n324=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n325=  }
n326=  if (%secs > 60) {
n327=    %my_duration = %my_duration $floor($calc(%secs / 60)) minutes
n328=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n329=  }
n330=  if (%secs != 0) {
n331=    %my_duration = %my_duration %secs seconds
n332=  }
n333=  return %my_duration
n334=}
