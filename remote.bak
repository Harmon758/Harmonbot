n93=on *:text:!level*:#:{
n94=  if ($2 isnum && $2 >= 1 && $2 < 127) {
n95=    var %2 = $floor($2)
n96=    var %level = 1
n97=    var %xp = 0
n98=    while (%level != %2) {
n99=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n100=      inc %level
n101=    }
n102=    %xp = $floor($calc(%xp / 4))
n103=    msg # Level %2 = $bytes(%xp,bd) xp
n104=  }
n105=  elseif ($2 isnum && $2 > 9000) { msg # It's over 9000! }
n106=  elseif ($2 isnum && $2 == 9000) { msg # Almost there. }
n107=  elseif ($2 isnum && $2 > 126 && $2 < 9000) { msg # I was gonna calculate xp at Level $2. Then I took an arrow to the knee. }
n108=  else { msg # Level $2- does not exist or there is a syntax error. }
n109=}
