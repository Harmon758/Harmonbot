n424=alias on/off {
n425=  if ($nick isop #) && ($2) {
n426=    if ($istok(on,$2,32)) {
n427=      set % $+ $3 $+ .status. $+ $chan on
n428=      if ($5) { msg # $5 is on }
n429=      else { msg # $3 is on }
n430=      return on
n431=    }
n432=    if ($istok(off,$2,32)) {
