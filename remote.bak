n336=on *:text:!uptime*:#:{
n337=  set %channel $chan
n338=  sockclose uptime
n339=  sockopen uptime nightdev.com 80
n340=}
n341=on *:sockopen:uptime:{
n342=  if ($sockerr) { sockclose $sockname | halt }
n343=  sockwrite -n $sockname GET /hosted/uptime.php?channel= $+ $mid(%channel,2-) HTTP/1.1
n344=  sockwrite -n $sockname Host: nightdev.com $crlf $+ $crlf
n345=}
n346=on *:sockread:uptime:{
n347=  if ($sockerr) { sockclose $sockname }
n348=  var %streamodata
n349=  sockread %streamodata
n350=  if ($regex(%streamodata,(.*)minutes)) { msg %channel $regml(1) minutes }
n351=  if ($regex(%streamodata,(.*)live.)) { msg %channel $capital($mid(%channel,2-)) is offline. }
n352=}
