n207=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n208=      inc %level
n209=    }
n210=    %xp = $floor($calc(%xp / 4))
n211=    msg # Level %2 = $bytes(%xp,bd) xp
n212=  }
n213=  elseif ($2 isnum && $2 > 9000) { msg # It's over 9000! }
n214=  elseif ($2 isnum && $2 == 9000) { msg # Almost there. }
n215=  elseif ($2 isnum && $2 > 126 && $2 < 9000) { msg # I was gonna calculate xp at Level $2. Then I took an arrow to the knee. }
n216=  else { msg # Level $2- does not exist or there is a syntax error. }
n217=}
