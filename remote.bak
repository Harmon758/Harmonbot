n318=alias my_duration {
n319=  var %secs = $1, %my_duration
n320=  if (%secs > 604800) {
n321=    %my_duration = %my_duration $floor($calc(%secs / 604800)) weeks
n322=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n323=  }
n324=  if (%secs > 86400) {
n325=    %my_duration = %my_duration $floor($calc(%secs / 86400)) days
n326=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n327=  }
n328=  if (%secs > 3600) {
n329=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hours
n330=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n331=  }
n332=  if (%secs > 60) {
n333=    %my_duration = %my_duration $floor($calc(%secs / 60)) minutes
n334=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n335=  }
n336=  if (%secs != 0) {
n337=    %my_duration = %my_duration %secs seconds
n338=  }
n339=  return %my_duration
n340=}
