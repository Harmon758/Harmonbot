n430=alias my_duration {
n431=  var %secs = $1, %my_duration
n432=  if (%secs > 604800) {
n433=    %my_duration = %my_duration $floor($calc(%secs / 604800)) weeks
n434=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n435=  }
n436=  if (%secs > 86400) {
n437=    %my_duration = %my_duration $floor($calc(%secs / 86400)) days
n438=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n439=  }
n440=  if (%secs > 3600) {
n441=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hours
n442=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n443=  }
n444=  if (%secs > 60) {
n445=    %my_duration = %my_duration $floor($calc(%secs / 60)) minutes
n446=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n447=  }
n448=  if (%secs != 0) {
n449=    %my_duration = %my_duration %secs seconds
n450=  }
n451=  return %my_duration
n452=}
