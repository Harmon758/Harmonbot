n422=alias on/off {
n423=  if ($nick isop #) && ($2) {
n424=    if ($istok(on,$2,32)) {
n425=      set % $+ $3 $+ .status. $+ $chan on
n426=      if ($5) { msg # $5 is on }
n427=      else { msg # $3 is on }
n428=      return on
n429=    }
n430=    if ($istok(off,$2,32)) {
n431=      set % $+ $3 $+ .status. $+ $chan off
n432=      if ($5) { msg # $5 is off }
n433=      else { msg # $3 is off }
n434=      return off
n435=    }
n436=    if ($istok(mod,$2,32)) {
n437=      set % $+ $3 $+ .status. $+ $chan mod
n438=      if ($5) { msg # $5 is mod only }
n439=      else { msg # $3 is mod only }
n440=      return mod
n441=    }
n442=  }
n443=  if (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == off) { return was_off }
n444=  elseif (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == mod) && ($nick isop #) {
n445=    msg # $4
n446=    return msg
n447=  }
n448=  elseif (% [ $+ [ $3 ] ] [ $+ .status. ] [ $+ [ $chan ] ] == mod) {
n449=    return was_mod_!msg
n450=  }
n451=  else {
n452=    msg # $4
n453=    return msg
n454=  }
n455=}
