n303=on *:text:!xpat*:#:{
n304=  var %2 = $remove($2,$chr(44))
n305=  if (!$2) { msg # Please enter xp. }
n306=  elseif (%2 isnum && %2 >= 0 && %2 < 200000001) {
n307=    var %2 = $floor(%2)
n308=    var %level = 1
n309=    var %xp = 0
n310=    while (%2 >= %xp) {
n311=      %xp = $calc(%xp * 4)
n312=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n313=      %xp = $calc(%xp / 4)
n314=      inc %level
n315=    }
n316=    %level = %level - 1
n317=    msg # $bytes(%2,bd) xp = level %level
n318=  }
n319=  elseif (%2 isnum) { msg # You can't have that much xp! }
n320=  else { msg # Syntax error. }
n321=}
n322=on *:text:!xpbetween*:#:{
n323=  if ($2 isnum && $2 >= 1 && $2 < 127 && $3 isnum && $3 >= 1 && $3 < 127 && $2 < $3) {
n324=    var %2 = $floor($2)
n325=    var %3 = $floor($3)
n326=    var %level = 1
n327=    var %xp = 0
n328=    var %startxp = 0
n329=    var %betweenxp = 0
n330=    while (%level != %3) {
n331=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n332=      inc %level
n333=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n334=    }
n335=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n336=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n337=  }
n338=  else { msg # Syntax error. }
n339=}
