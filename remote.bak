n354=alias timeleft {
n355=  var %start = $1, %interval = $2, %timeleft
n356=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n357=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n358=  return %timeleft
n359=}
n360=
n361=alias my_duration {
n362=  var %secs = $1, %my_duration
n363=  if (%secs > 604800) {
n364=    %my_duration = %my_duration $floor($calc(%secs / 604800)) weeks
n365=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n366=  }
n367=  if (%secs > 86400) {
n368=    %my_duration = %my_duration $floor($calc(%secs / 86400)) days
n369=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n370=  }
n371=  if (%secs > 3600) {
n372=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hours
n373=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n374=  }
n375=  if (%secs > 60) {
n376=    %my_duration = %my_duration $floor($calc(%secs / 60)) minutes
n377=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n378=  }
n379=  if (%secs != 0) {
n380=    %my_duration = %my_duration %secs seconds
n381=  }
n382=  return %my_duration
n383=}
