n386=alias my_duration {
n387=  var %secs = $1, %my_duration
n388=  if (%secs > 31536000) {
n389=    %my_duration = %my_duration $floor($calc(%secs / 31536000)) year
n390=    if ($floor($calc(%secs / 31536000)) > 1) { %my_duration = %my_duration $+ s }
n391=    %secs = $calc(%secs - $floor($calc(%secs / 31536000)) * 31536000)
n392=  }
n393=  if (%secs > 604800) {
n394=    %my_duration = %my_duration $floor($calc(%secs / 604800)) week
n395=    if ($floor($calc(%secs / 604800)) > 1) { %my_duration = %my_duration $+ s }
n396=    %secs = $calc(%secs - $floor($calc(%secs / 604800)) * 604800)
n397=  }
n398=  if (%secs > 86400) {
n399=    %my_duration = %my_duration $floor($calc(%secs / 86400)) day
n400=    if ($floor($calc(%secs / 86400)) > 1) { %my_duration = %my_duration $+ s }
n401=    %secs = $calc(%secs - $floor($calc(%secs / 86400)) * 86400)
n402=  }
n403=  if (%secs > 3600) {
n404=    %my_duration = %my_duration $floor($calc(%secs / 3600)) hour
n405=    if ($floor($calc(%secs / 3600)) > 1) { %my_duration = %my_duration $+ s }
n406=    %secs = $calc(%secs - $floor($calc(%secs / 3600)) * 3600)
n407=  }
n408=  if (%secs > 60) {
n409=    %my_duration = %my_duration $floor($calc(%secs / 60)) minute
n410=    if ($floor($calc(%secs / 60)) > 1) { %my_duration = %my_duration $+ s }
n411=    %secs = $calc(%secs - $floor($calc(%secs / 60)) * 60)
n412=  }
n413=  if (%secs != 0) {
n414=    %my_duration = %my_duration %secs second
n415=    if (%secs > 1) { %my_duration = %my_duration $+ s }
n416=  }
n417=  return %my_duration
n418=}
