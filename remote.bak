n191=  if (!$2) { msg # Please enter a level. }
n192=  elseif ($2 isnum && $2 >= 1 && $2 < 127) {
n193=    var %2 = $floor($2)
n194=    var %level = 1
n195=    var %xp = 0
n196=    while (%level != %2) {
n197=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n198=      inc %level
n199=    }
n200=    %xp = $floor($calc(%xp / 4))
n201=    msg # Runescape Level %2 = $bytes(%xp,bd) xp
n202=  }
n203=  elseif ($2 isnum && $2 > 9000) { msg # It's over 9000! }
n204=  elseif ($2 isnum && $2 == 9000) { msg # Almost there. }
n205=  elseif ($2 isnum && $2 > 126 && $2 < 9000) { msg # I was gonna calculate xp at Level $2. Then I took an arrow to the knee. }
