n174=on *:text:!level*:#:{
n175=  if (!$2) { msg # Please enter a level. }
n176=  elseif ($2 isnum && $2 >= 1 && $2 < 127) {
n177=    var %2 = $floor($2)
n178=    var %level = 1
n179=    var %xp = 0
n180=    while (%level != %2) {
n181=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n182=      inc %level
n183=    }
n184=    %xp = $floor($calc(%xp / 4))
n185=    msg # Level %2 = $bytes(%xp,bd) xp
n186=  }
n187=  elseif ($2 isnum && $2 > 9000) { msg # It's over 9000! }
n188=  elseif ($2 isnum && $2 == 9000) { msg # Almost there. }
n189=  elseif ($2 isnum && $2 > 126 && $2 < 9000) { msg # I was gonna calculate xp at Level $2. Then I took an arrow to the knee. }
n190=  else { msg # Level $2- does not exist or there is a syntax error. }
n191=}
