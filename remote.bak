n292=on *:sockopen:uptime:{
n293=  if ($sockerr) { sockclose $sockname | halt }
n294=  sockwrite -n $sockname GET /hosted/uptime.php?channel=mikkiscape HTTP/1.1
n295=  sockwrite -n $sockname Host: nightdev.com $crlf $+ $crlf
n296=}
n297=on *:sockread:uptime:{
n298=  if ($sockerr) { sockclose $sockname }
n299=  var %streamodata
n300=  sockread %streamodata
n301=  if ($regex(%streamodata,(.*)minutes)) { msg #mikkiscape $regml(1) minutes }
n302=  if ($regex(%streamodata,(.*)live.)) { msg #mikkiscape Mikki is offline. }
n303=}
