n202=    while (%level != %3) {
n203=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n204=      inc %level
n205=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n206=    }
n207=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n208=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n209=  }
n210=  else { msg # Syntax error. }
n211=}
