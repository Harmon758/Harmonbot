n297=on *:text:!xpat*:#:{
n298=  var %2 = $remove($2,$chr(44))
n299=  if (!$2) { msg # Please enter xp. }
n300=  elseif (%2 isnum && %2 >= 0 && %2 < 200000001) {
n301=    var %2 = $floor(%2)
n302=    var %level = 1
n303=    var %xp = 0
n304=    while (%2 >= %xp) {
n305=      %xp = $calc(%xp * 4)
n306=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n307=      %xp = $calc(%xp / 4)
n308=      inc %level
n309=    }
n310=    %level = %level - 1
n311=    msg # $bytes(%2,bd) xp = level %level
n312=  }
n313=  elseif (%2 isnum) { msg # You can't have that much xp! }
n314=  else { msg # Syntax error. }
n315=}
n316=on *:text:!xpbetween*:#:{
n317=  if ($2 isnum && $2 >= 1 && $2 < 127 && $3 isnum && $3 >= 1 && $3 < 127 && $2 < $3) {
n318=    var %2 = $floor($2)
n319=    var %3 = $floor($3)
n320=    var %level = 1
n321=    var %xp = 0
n322=    var %startxp = 0
n323=    var %betweenxp = 0
n324=    while (%level != %3) {
n325=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n326=      inc %level
n327=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n328=    }
n329=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n330=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n331=  }
n332=  else { msg # Syntax error. }
n333=}
