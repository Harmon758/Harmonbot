n348=    var %betweenxp = 0
n349=    while (%level != %3) {
n350=      %xp = $calc(%xp + $floor($calc(%level + 300 * 2 ^ (%level / 7))))
n351=      inc %level
n352=      if (%level == %2) { %startxp = $floor($calc(%xp / 4)) }
n353=    }
n354=    %betweenxp = $calc($floor($calc(%xp / 4)) - %startxp)
n355=    msg # $bytes(%betweenxp,bd) xp between level %2 and level %3
n356=  }
n357=  else { msg # Syntax error. }
n358=}
