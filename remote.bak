n305=alias timeleft {
n306=  var %start = $1, %interval = $2, %timeleft
n307=  if ($ctime <= %start) || (!%interval) { %timeleft = $calc(%start - $ctime) }
n308=  else { %timeleft = $calc(%interval - ($ctime - %start) % %interval) }
n309=  return %timeleft
n310=}
