on $*:text:/(3 accs|3 accounts|three accs|three accounts)/Si:#mikkiscape:{
  if ($nick isop #) && ($2) {
    if ($istok(on,$3,32)) {
      set %3accs.status on 
      msg # 3 accs is on.
      return
    }
    if ($istok(off,$3,32)) {
      set %3accs.status off 
      msg # 3 accs is off.
      return
    }
  }
  if (%3accs.status == off) { return }
  else {
    inc %3accs
    msg # Yes, Mikki is playing 3 accounts. This question has been asked %3accs times.
  }
}

on *:text:*alt*:#mikkiscape:{
  if ($nick isop #) && ($2) {
    if ($istok(on,$2,32)) {
      set %alt.status on 
      msg # alt is on.
      return
    }
    if ($istok(off,$2,32)) {
      set %alt.status off 
      msg # alt is off.
      return
    }
  }
  if (%alt.status == off) { return }
  else {
    msg # Bad $capital($nick) $+ !
  }
}

